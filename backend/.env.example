# PDFXcel Backend Environment Variables
# Copy this file to .env and fill in your actual values

# Anthropic Claude AI API Key (Required)
ANTHROPIC_API_KEY=sk-ant-api03-your-actual-key-here

# Application Settings
ENVIRONMENT=production
LOG_LEVEL=INFO
DEBUG=false

# File Upload Settings
MAX_FILE_SIZE=10485760
ALLOWED_EXTENSIONS=pdf

# Cleanup Settings
CLEANUP_INTERVAL_HOURS=24
CLEANUP_AGE_HOURS=48

# Server Settings (Railway sets these automatically)
PORT=8000

# CORS Settings (Optional)
# ALLOWED_ORIGINS=https://your-flutter-app-domain.com

# Redis URL (Optional - for caching)
# REDIS_URL=redis://redis-service-url